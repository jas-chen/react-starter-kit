{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "description": "React Starter Kit",
  "private": true,
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "pretest": "rm -rf coverage",
    "test": "NODE_ENV=test karma start",
    "posttest": "cat coverage/**/coverage.txt",
    "lint:js": "eslint src",
    "lint:css": "stylelint 'src/**/*.scss' --syntax scss",
    "lint": "npm run lint:js && npm run lint:css",
    "watch": "NODE_ENV=development babel-node server/app.js",
    "start": "NODE_ENV=production node server/static.js",
    "api": "node server/api.js",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack/config.prod.js --progress --verbose --colors --display-error-details && babel-node scripts/write-html-file.js",
    "postbuild": "rm dist/stats.json",
    "precommit": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jas-chen/react-starter-kit.git"
  },
  "author": "Jas Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jas-chen/react-starter-kit/issues"
  },
  "homepage": "https://github.com/jas-chen/react-starter-kit#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.18.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.8.5",
    "husky": "^0.11.4",
    "jsdom": "^8.4.0",
    "json-server": "^0.8.10",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.8.2",
    "proxy-middleware": "^0.15.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "stylelint": "^6.2.2",
    "stylelint-config-standard": "^6.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-stats-plugin": "^0.1.1",
    "whatwg-fetch": "^0.11.0"
  }
}
